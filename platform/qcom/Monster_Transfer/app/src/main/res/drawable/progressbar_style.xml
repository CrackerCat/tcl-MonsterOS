<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8.0dip" />
            <gradient android:startColor="#e5e5e5" android:endColor="#e5e5e5"
                android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#95adeb" android:endColor="#95adeb"
                    android:centerColor="#95adeb" android:angle="270.0" />
            </shape>
        </clip>
    </item>

</layer-list>