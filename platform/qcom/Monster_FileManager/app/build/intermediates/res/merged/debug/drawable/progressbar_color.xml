<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/progressbar_background"
                    android:startColor="@color/progressbar_background" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/progressbar_progress"
                    android:startColor="@color/progressbar_progress" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/progressbar_progress"
                    android:startColor="@color/progressbar_progress" />
            </shape>
        </clip>
    </item>
</layer-list>