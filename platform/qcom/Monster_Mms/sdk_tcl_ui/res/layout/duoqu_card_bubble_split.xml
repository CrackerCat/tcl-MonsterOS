<?xml version="1.0" encoding="utf-8"?>
<cn.com.xy.sms.sdk.ui.popu.widget.DuoquRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/duoqu_card_body_bottom"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="4dp"
    android:paddingTop="4dp" >

    <TextView
        android:id="@+id/duoqu_call_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:includeFontPadding="false"
        android:textSize="10dp" />

    <ImageView
        android:id="@+id/duoqu_tel_card"
        android:layout_width="8dp"
        android:layout_height="10dp"
        android:layout_marginRight="2dp"
        android:layout_toLeftOf="@id/duoqu_call_name"
        android:layout_centerVertical="true" />

    <TextView
        android:id="@+id/duoqu_call_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:includeFontPadding="false"
        android:layout_toLeftOf="@id/duoqu_tel_card"
        android:textSize="10dp" />

</cn.com.xy.sms.sdk.ui.popu.widget.DuoquRelativeLayout>