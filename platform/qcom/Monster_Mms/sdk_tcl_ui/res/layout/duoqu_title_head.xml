<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="334dp"
    android:layout_height="60dp" >

    <TextView
        android:id="@+id/duoqu_title_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/duoqu_ui_part_titile_head_tv_margin_start"
        android:textSize="@dimen/duoqu_ui_part_titile_head_text_size"
        android:textColor="@color/duoqu_theme_color_4010"
        android:layout_centerVertical="true"/>
    <ImageView
        android:id="@+id/duoqu_left_logo"
        android:layout_width="@dimen/duoqu_ui_part_titile_head_img_width"
        android:layout_height="@dimen/duoqu_ui_part_titile_head_img_height"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="@dimen/duoqu_ui_part_titile_head_img_marginleft"
        android:layout_centerVertical="true"/>


</RelativeLayout>
