<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/duoqu_ui_part_sms_orgin_paddingleft"
    android:paddingRight="@dimen/duoqu_ui_part_sms_orgin_paddingright"
    android:paddingTop="@dimen/duoqu_ui_part_sms_orgin_paddingtop"
    android:paddingBottom="@dimen/duoqu_ui_part_sms_orgin_paddingbottom"
    android:orientation="vertical" >
 
    <TextView
        android:id="@+id/duoqu_sms_origin_text"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:textSize="@dimen/duoqu_ui_part_sms_orgin_tv_textsize" />
 
    <TextView
        android:id="@+id/duoqu_sms_origin_text_hint"
        android:layout_width="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_height="match_parent"
        android:textSize="@dimen/duoqu_ui_part_sms_orgin_tv_hint_size" />
        
</LinearLayout>
